# Release TBB recording on a bunch of stations
#
########################################################################

import logging
from lofar.mac.tbb.tbb_release_recording import main

if __name__ == '__main__':
    format = '%(asctime)-15s | %(message)s'
    logging.basicConfig(format=format, level=logging.INFO)
    main()
